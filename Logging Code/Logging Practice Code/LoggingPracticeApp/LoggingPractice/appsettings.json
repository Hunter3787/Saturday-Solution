{ // better practive to configure logging here than in c# code.
  "Logging": {
    "Console": {
      "Loglevel": {
        "Default": "Informaton",
        "Microsoft*": "Warning"
      }
    },
    "Debug": {
      "Loglevel": {
        "Default": "Warning",
        "Microsoft*": "Warning"
      }
    },
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Warning",
      "Microsoft*": "Warning"
    }
  },
  "AllowedHosts": "*"
}
